{{#each atendimentos}}
<div class="card mt-4">
    <div class="card-body">
        <h3>{{titulo}}</h3>
        <p>{{descricao}}</p>
        <a href="/atendimentos/{{_id}}"><button class="btn btn-success">Leia mais</button></a>
        <hr>
        <small>{{data}}</small>
    </div>
</div>
{{else}}
    <h5>Nenhum atendimento cadastrado</h5>
{{/each}}